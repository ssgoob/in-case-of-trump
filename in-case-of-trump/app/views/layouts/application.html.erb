<!DOCTYPE html>
<html>
<head>
  <title>In Case Of Trump</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
    <% if current_user %>          
      <li><%= link_to 'Log Out', logout_path %></li><br> 
    <% else %> 
      <li><%= link_to 'Sign Up', signup_path %></li><br>
      <li><%= link_to 'Log In', login_path %></li><br>
    <% end %> 

</head>
<body>

<%= yield %>

</body>
</html>
