<%= f.label :name %>
<%= f.text_field :name, placeholder: "Enter name" %><br>

<%= f.label :dob %>
<%= f.date_select :dob, start_year: Time.now.year - 100, end_year: Time.now.year %><br>

<%= f.label :gender %>
<%= f.select :gender, ['M', 'F', 'Q', 'T'] %><br>

<%= f.label :status %>
<%= f.select :status, ['Citizen', 'International'] %><br>

<%= f.label :preference %>
<%= f.select :preference, ['M', 'F', 'Q', 'T'] %><br>

<%= f.label :email %>
<%= f.select :email, placeholder: "Enter email address" %><br>